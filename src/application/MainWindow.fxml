<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainWindowController">
   <children>
      <Button fx:id="Connect" layoutX="238.0" layoutY="287.0" mnemonicParsing="false" onAction="#Connect" rotate="-1.0" text="Connect" />
      <Label fx:id="lblText" layoutX="188.0" layoutY="268.0" prefHeight="17.0" prefWidth="127.0" rotate="1.0" />
      <Button fx:id="btnNeuesFenster" alignment="CENTER" contentDisplay="TOP" layoutX="102.0" layoutY="242.0" mnemonicParsing="false" onAction="#handlebtnNeuesFensterAction" text="Connect" AnchorPane.leftAnchor="102.0" AnchorPane.rightAnchor="98.0" />
      <Button fx:id="new_contact" layoutX="208.0" layoutY="30.0" mnemonicParsing="false" onAction="#new_contact" text="New Contact" />
      <Button fx:id="remove_contact" layoutX="425.0" layoutY="98.0" mnemonicParsing="false" onAction="#remove_contact" prefHeight="25.0" prefWidth="38.0" text="X" />
      <ComboBox fx:id="existing_contacts" layoutX="102.0" layoutY="200.0" prefHeight="25.0" prefWidth="300.0" promptText="Contacts" />
      <TextField fx:id="new_name" alignment="CENTER" layoutX="176.0" layoutY="73.0" promptText="Name" />
      <TextField fx:id="new_ip" alignment="CENTER" layoutX="178.0" layoutY="111.0" promptText="IP" />
   </children>
</AnchorPane>
